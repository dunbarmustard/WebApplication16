@{
    ViewData["Title"] = "Supplier Retail Results";
}

@using WebApplication16.Models
@model WebApplication16.Models.SupplierRetailResults




<h1>@ViewData["Title"]</h1>

<a class="btn btn-lg btn-success" asp-controller="Home" asp-action="Index">
    <i class="fa fa-home fa-2x pull-left">GO BACK</i>
</a>





<body>





    <div style="height:700px;overflow:auto; width:1500px">



        <table cellpadding="0" cellspacing="0">
            <tr>
                <th>Supplier ID</th>
                <th>Company Name</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>License#</th>
                <th>Phone </th>
                <th>BookID</th>
                <th>Book Name</th>
                <th>ISBN </th>
                <th>Cost </th>
                <th>Stock Count </th>
            </tr>






            @foreach (var info in Model.qResult)
            {
                <tr>
                    <td>@info.supplierid</td>
                    <td>@info.companyname</td>
                    <td>@info.address</td>
                    <td>@info.city</td>
                    <td>@info.state</td>
                    <td>@info.licenseno</td>

                    <td>@info.phone</td>
                    <td>@info.bookid</td>
                    <td>@info.bookname</td>
                    <td>@info.isbn</td>
                    <td>@info.cost</td>
                    <td>@info.stockcount</td>

                </tr>
            }
        </table>
    </div>

</body>







